<form action="<%= users_url %>" method="post">

  <label for="user_name">Username</label>
  <input
    type="text"
    name="user[username]"
    id="user_username">

  <label for="user_password">Password</label>
  <input
    type="password"
    name="user[password]"
    id="user_password">
  <br>

  <input type='submit' value='Register'>
</form>
